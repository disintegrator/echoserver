{
  "variables": {
  },
  "builders": [{
    "type": "docker",
    "image": "node:4-slim",
    "export_path": "image.tar"
  }],
  "provisioners": [
    {
      "type": "file",
      "source": ".",
      "destination": "/app"
    },
    {
      "type": "shell",
      "inline": [
        "echo Install app dependencies",
        "cd /app",
        "npm install"
      ]
    }
  ]
}
